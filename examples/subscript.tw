sub& sub_ref [x: i64] -> i64 =
  yield &x;

sub&mut sub_refmut [mut x: i64] -> i64 =
  yield &mut x;

sub sub_value [x: i64] -> i64 =
  yield x;

fn main = (
  let value = 100;

  (
    let ref = sub_ref[value]
  );

  (
    let refmut = sub_refmut[value]
  );

  (
    let val = sub_value[value];
    let valmut = mut sub_value[value]
  )
);
